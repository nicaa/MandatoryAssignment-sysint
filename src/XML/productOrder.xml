<ns1:PurchaseOrder
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://xml.netbeans.org/schema/ProductOrder'
    xsi:schemaLocation='http://xml.netbeans.org/schema/ProductOrder ProductOrder.xsd'>
    <ns1:ShippingInformation>
        <ns1:Street>Stairway</ns1:Street>
        <ns1:PostalCode>2400</ns1:PostalCode>
        <ns1:City>Copenhagen</ns1:City>
        <ns1:Country>Denmark</ns1:Country>
        <ns1:FirstName>Rasmus</ns1:FirstName>
        <ns1:LastName>Hojte</ns1:LastName>
    </ns1:ShippingInformation>
    <ns1:Order>
        <ns1:Item>
            <ns1:Product>Apple</ns1:Product>
            <ns1:Price>50</ns1:Price>
        </ns1:Item>
        <ns1:Item>
            <ns1:Product>Banana</ns1:Product>
            <ns1:Price>50</ns1:Price>
        </ns1:Item>
        
        <ns1:TotalPrice>100</ns1:TotalPrice>
    </ns1:Order>

</ns1:PurchaseOrder>
